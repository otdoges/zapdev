{
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "rewrites": [
    {
      "source": "/api/stripe/webhook",
      "destination": "/api/stripe/webhook"
    }
  ],
  "headers": [
    {
      "source": "/api/stripe/webhook",
      "headers": [
        {
          "key": "X-Vercel-Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}