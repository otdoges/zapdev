{
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.zapdev.link"
        }
      ],
      "destination": "https://zapdev.link/:path*",
      "permanent": true,
      "statusCode": 308
    }
  ]
}
